{
  "browserslist": [
    "ie 11"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@frctl/fractal": "^1.3.0",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "autoprefixer": "^9.8.6",
    "browserslist": "^4.13.0",
    "eslint": "^7.5.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "rollup": "^2.23.0",
    "rollup-plugin-postcss": "^3.1.3",
    "rollup-plugin-terser": "^6.1.0",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "tailwindcss": "^1.6.0"
  },
  "scripts": {
    "assets-build": "NODE_ENV=production rollup --config",
    "assets-start": "NODE_ENV=development rollup --config --watch",
    "styleguide-start": "fractal start --sync --watch",
    "styleguide-build": "fractal build",
    "start": "yarn assets-start & yarn styleguide-start",
    "build": "yarn assets-build && yarn styleguide-build",
    "js-lint": "eslint --config .eslintrc.json main.js 'components/**/*.js'",
    "js-fix": "eslint --fix --config .eslintrc.json main.js 'components/**/*.js'",
    "css-lint": "stylelint 'components/**/*.css'",
    "lint": "yarn js-lint && yarn css-lint"
  }
}
